/* LOOP COMPILER */

int x;
x = 15;

func int addValue(int num1 int num2){
   int result;
   result = num1 + num2;
   return result;
}

func int main(){
   int result1;
   while (x > 10){
      result1 = addValue(x 1);
      swrite "result:";
      write result1;
      writeln;
      x = x - 1;
      swrite "Onto Next Iteration";
      writeln;
   }
   return 0;
}
